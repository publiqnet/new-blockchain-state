{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "url": "git@github.com:publiqnet/publiq-api.git",
            "type": "git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/orm": "^2.6",
        "nelmio/api-doc-bundle": "^3.3",
        "nelmio/cors-bundle": "^1.5",
        "presta/sitemap-bundle": "^1.7",
        "publiq/publiq-api": "1.0.x-dev",
        "sensio/framework-extra-bundle": "^5.2",
        "sonata-project/admin-bundle": "^3.53",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "sonata-project/translation-bundle": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.3",
        "sybio/image-workshop": "^2.1",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/lock": "*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/property-access": "*",
        "symfony/security-bundle": "4.1.*",
        "symfony/security-core": "4.1.*",
        "symfony/security-guard": "4.1.*",
        "symfony/serializer": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "4.1.*",
        "symfony/yaml": "*",
        "tooleks/php-avg-color-picker": "^1.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/dotenv": "*",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.1.*"
        }
    }
}
